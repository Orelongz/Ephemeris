{
  "root": true,
  "extends": [
    "plugin:react/recommended",
    "eslint-config-airbnb-base"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "quotes": ["error", "single"]
  },
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": {
          "resolve": {
            "modules": ["frontend", "node_modules"]
          }
        }
      }
    }
  }
}
